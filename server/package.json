    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "eslint": "^9.9.1",
    "eslint-config-standard-ext": "^1.1.0",
    "fastify-tsconfig": "^2.0.0",
    "typescript": "^5.5.4"
  }
